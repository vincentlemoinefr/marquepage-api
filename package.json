{
  "name": "marquepage-api",
  "version": "1.0.2",
  "description": "Synchronise bookmarks between browsers and share them",
  "main": "./src/server.js",
  "scripts": {
    "start": "node .",
    "test": "node --env-file=.env .",
    "api:build:schema": "node ./tooling/api_build_json_schema.js",
    "api:build:controllers": "node ./tooling/api_build_controllers.js",
    "api:build:documentation": "node ./tooling/api_build_documentation.js",
    "api:show:config": "node --env-file=.env ./src/config/index.js",
    "docker:build": "sh ./tooling/docker_build.sh",
    "docker:nuke": "sh ./tooling/docker_nuke.sh",
    "docker:exec:api": "sh ./tooling/docker_exec.sh api",
    "docker:exec:db": "sh ./tooling/docker_exec.sh db"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vincentlemoinefr/marquepage-api.git"
  },
  "author": "vincent.lemoine.fr@gmail.com",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/vincentlemoinefr/marquepage-api/issues"
  },
  "homepage": "https://github.com/vincentlemoinefr/marquepage-api#readme",
  "dependencies": {
    "express": "^4.19.2",
    "joi": "^17.13.3",
    "mongodb": "^6.8.0",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "yaml": "^2.5.0"
  }
}
